{
  "name": "flex-tool-bar",
  "main": "./lib/flex-tool-bar",
  "version": "2.1.3",
  "description": "Easily Customizable ToolBar for Atom",
  "repository": "https://github.com/cakecatz/flex-toolbar",
  "license": "MIT",
  "keywords": [
    "toolbar",
    "tool-bar"
  ],
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "change-case": "^3.1.0",
    "cson": "^5.1.0",
    "fs-plus": "^3.1.1",
    "json5": "^2.1.0",
    "glob-to-regexp": "0.4.1"
  },
  "package-deps": [
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "atomTestRunner": "./spec/runner",
  "scripts": {
    "test": "atom --test ./spec && npm run lint",
    "lint": "remark . & eslint .",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^6.0.1",
    "atom-jasmine3-test-runner": "^4.3.1",
    "eslint": "^5.16.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "semantic-release": "^15.13.12"
  }
}
