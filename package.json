{
  "name": "flex-tool-bar",
  "main": "./lib/flex-tool-bar",
  "version": "2.0.0",
  "description": "Easily Customizable ToolBar for Atom",
  "repository": "https://github.com/cakecatz/flex-toolbar",
  "license": "MIT",
  "keywords": [
    "toolbar",
    "tool-bar"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.6.1",
    "change-case": "^3.0.1",
    "chokidar": "^2.0.2",
    "cson": "^5.1.0",
    "fs-plus": "^3.0.2",
    "json5": "^0.5.1",
    "glob-to-regexp": "0.4.0"
  },
  "package-deps": [
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "atomTestRunner": "atom-jasmine3-test-runner",
  "scripts": {
    "test": "atom --test ./spec && npm run lint",
    "lint": "remark . & eslint ."
  },
  "devDependencies": {
    "atom-jasmine3-test-runner": "^3.1.0",
    "eslint": "^4.18.2",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.1"
  }
}
